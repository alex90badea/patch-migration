{
    "name": "alex90badea/patch-migration",
    "description": "As Laravel migrations work, this package allows you to keep track of code migrations and execute code blocks as if they are migrations.",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Badea",
            "email": "alex90badea@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/console": "^7.0",
        "illuminate/database": "^7.0",
        "illuminate/filesystem": "^7.0",
        "illuminate/support": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "AlexBadea\\PatchMigration\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AlexBadea\\PatchMigration\\\\PatchMigrationServiceProvider"
            ]
        }
    }
}
